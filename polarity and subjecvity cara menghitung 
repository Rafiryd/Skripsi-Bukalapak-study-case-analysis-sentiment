    {
      "cell_type": "code",
      "source": [
        "import pandas as pd\n",
        "from afinn import Afinn\n",
        "from textblob import TextBlob\n",
        "\n",
        "# Sample DataFrame with a 'text' column\n",
        "data = {\n",
        "    'text': [\n",
        "        \"I love this product. It's amazing!\",\n",
        "        \"The weather is terrible today.\",\n",
        "        \"This book is boring.\",\n",
        "        \"The concert was fantastic.\",\n",
        "        \"I had a bad experience at the restaurant.\",\n",
        "        \"She is very friendly and helpful.\",\n",
        "        \"The movie made me cry.\",\n",
        "        \"The team won the championship!\",\n",
        "        \"The traffic jam was frustrating.\",\n",
        "        \"His speech was inspiring and uplifting.\"\n",
        "    ]\n",
        "}\n",
        "\n",
        "# Create a pandas DataFrame\n",
        "df = pd.DataFrame(data)\n",
        "\n",
        "# Initialize the AFINN sentiment analyzer\n",
        "afinn = Afinn()\n",
        "\n",
        "# Iterate through each row in the DataFrame\n",
        "for index, row in df.iterrows():\n",
        "    text = row['text']\n",
        "\n",
        "    print(f\"Document {index + 1}:\")\n",
        "    print(text)\n",
        "\n",
        "    # Split the sentence into words\n",
        "    words = text.split()\n",
        "\n",
        "    # Initialize the overall sentiment score for the document\n",
        "    document_sentiment = 0\n",
        "\n",
        "    # Analyze each word using AFINN and print its sentiment value\n",
        "    for word in words:\n",
        "        word_score = afinn.score(word)\n",
        "        document_sentiment += word_score\n",
        "        print(f\"{word}: {word_score:.2f}\")\n",
        "\n",
        "    # Perform polarity and subjectivity analysis using TextBlob\n",
        "    tb = TextBlob(text)\n",
        "    polarity = tb.sentiment.polarity\n",
        "    subjectivity = tb.sentiment.subjectivity\n",
        "\n",
        "    # Determine sentiment category (negative, neutral, or positive)\n",
        "    if polarity < 0:\n",
        "        sentiment_category = \"Negative\"\n",
        "    elif polarity == 0:\n",
        "        sentiment_category = \"Neutral\"\n",
        "    else:\n",
        "        sentiment_category = \"Positive\"\n",
        "\n",
        "    # Print polarity, subjectivity, and sentiment category\n",
        "    print(f\"Polarity: {polarity:.2f}\")\n",
        "    print(f\"Subjectivity: {subjectivity:.2f}\")\n",
        "    print(f\"Sentiment: {sentiment_category}\")\n",
        "\n",
        "    # Print the overall sentiment score for the document\n",
        "    print(f\"Overall Document Sentiment: {document_sentiment:.2f}\\n\")\n"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "CrBp50wly7Px",
        "outputId": "bf5c1286-f893-4a44-9f72-ec6def997e42"
      },
      "execution_count": 6,
      "outputs": [
        {
          "output_type": "stream",
