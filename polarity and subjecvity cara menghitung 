      "source": [
        "from afinn import Afinn\n",
        "\n",
        "# Sample list of sentences for analysis\n",
        "sentences = [\n",
        "    \"I love this product. It's amazing!\",\n",
        "    \"The weather is terrible today.\",\n",
        "    \"This book is boring.\",\n",
        "    \"The concert was fantastic.\",\n",
        "    \"I had a bad experience at the restaurant.\",\n",
        "    \"She is very friendly and helpful.\",\n",
        "    \"The movie made me cry.\",\n",
        "    \"The team won the championship!\",\n",
        "    \"The traffic jam was frustrating.\",\n",
        "    \"His speech was inspiring and uplifting.\"\n",
        "]\n",
        "\n",
        "# Initialize the AFINN sentiment analyzer\n",
        "afinn = Afinn()\n",
        "\n",
        "# Iterate through each sentence\n",
        "for i, sentence in enumerate(sentences, start=1):\n",
        "    print(f\"Document {i}:\")\n",
        "    print(sentence)\n",
        "\n",
        "    # Split the sentence into words\n",
        "    words = sentence.split()\n",
        "\n",
        "    # Analyze each word and print its sentiment value\n",
        "    for word in words:\n",
        "        sentiment_score = afinn.score(word)\n",
        "        print(f\"{word}: {sentiment_score:.2f}\")\n",
        "\n",
        "    # Calculate and print the overall sentiment of the document\n",
        "    document_sentiment = afinn.score(sentence)\n",
        "    print(f\"Overall Document Sentiment: {document_sentiment:.2f}\\n\")\n"
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "from textblob import TextBlob\n",
        "\n",
        "# Define functions for subjectivity, polarity, and sentiment analysis\n",
        "def getSubjectivity(review):\n",
        "    return TextBlob(review).sentiment.subjectivity\n",
        "\n",
        "def getPolarity(review):\n",
        "    return TextBlob(review).sentiment.polarity\n",
        "\n",
        "def analyze(score):\n",
        "    if score < 0:\n",
        "        return 'Negative'\n",
        "    elif score == 0:\n",
        "        return 'Neutral'\n",
        "    else:\n",
        "        return 'Positive'\n",
        "\n",
        "# Sample list of sentences for analysis\n",
        "sentences = [\n",
        "    \"I love this product. It's amazing!\",\n",
        "    \"The weather is terrible today.\",\n",
        "    \"This book is boring.\",\n",
        "    \"The concert was fantastic.\",\n",
        "    \"I had a bad experience at the restaurant.\",\n",
        "    \"She is very friendly and helpful.\",\n",
        "    \"The movie made me cry.\",\n",
        "    \"The team won the championship!\",\n",
        "    \"The traffic jam was frustrating.\",\n",
        "    \"His speech was inspiring and uplifting.\"\n",
        "]\n",
        "\n",
        "# Iterate through each sentence and perform sentiment analysis\n",
        "for i, sentence in enumerate(sentences, start=1):\n",
        "    print(f\"Document {i}:\")\n",
        "    print(sentence)\n",
        "\n",
        "    # Get sentiment scores using TextBlob\n",
        "    subjectivity_score = getSubjectivity(sentence)\n",
        "    polarity_score = getPolarity(sentence)\n",
        "\n",
        "    # Analyze sentiment\n",
        "    subjectivity_label = analyze(subjectivity_score)\n",
        "    polarity_label = analyze(polarity_score)\n",
        "\n",
        "    print(f\"Subjectivity Score: {subjectivity_score:.2f}\")\n",
        "    print(f\"Polarity Score: {polarity_score:.2f}\")\n",
        "    print(f\"Subjectivity Label: {subjectivity_label}\")\n",
        "    print(f\"Polarity Label: {polarity_label}\\n\")\n"
      ],
      "metadata": {
        "colab": {
